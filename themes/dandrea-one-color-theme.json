{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"editor.background": "#101010",
		"editor.foreground": "#C0C0C0",
		"editor.lineHighlightBackground": "#161616",
		"editor.selectionBackground": "#143D66",
		"editorCursor.foreground": "#f0f0f0",
		"editorIndentGuide.activeBackground": "#204060a0",
		"editorWhitespace.foreground": "#202020"
	},
	"tokenColors": [
		{
			"name": "Comment", "scope": "comment",
			"settings": { "foreground": "#60B040" }
		}, {
			"name": "Punctuation", "scope": ["punctuation.definition.arguments", "punctuation.definition.parameters"],
			"settings": { "foreground": "#C0C0C080", "fontStyle": "bold" }
		}, {
			"name": "String", "scope": ["string.quoted.double"],
			"settings": { "foreground": "#2080F0" }
		}, {
			"name": "String", "scope": ["string.quoted.single"],
			"settings": { "foreground": "#2060F0" }
		}, {
			"name": "String comment", "scope": "string.quoted.docstring",
			"settings": { "foreground": "#48a723", "fontStyle": "italicbold" }
		}, {
			"name": "Number", "scope": "constant.numeric",
			"settings": { "foreground": "#ff80b0" }
		}, {
			"name": "Enum Member", "scope": "*.variable.enum-member",
			"settings": { "foreground": "#ff80b0" }
		}, {
		// 	*"name": "Constant other", "scope": ["constant.other"],
		// 	"settings": { "foreground": "#ff5040", "fontStyle": "" }
		// }, {
			"name": "Constant character", "scope": ["constant.character"],
			"settings": { "foreground": "#d060f0", "fontStyle": "" }
		}, {
			"name": "Variable", "scope": "variable",
			"settings": { "foreground": "#A0C0ff", "fontStyle": ""}
		}, {
			"name": "Variable other",
			"scope":"variable.other.readwrite",
			"settings": { "foreground": "#A0C0ff", "fontStyle": "" }
		}, {
			"name": "Variable other", "scope":"variable.other.object",
			"settings": { "foreground": "#C0F0A0", "fontStyle": "" }
		}, {
			"name":"Property",
			"scope":"variable.other.object.property",
			"settings": { "foreground": "#DACA80", "fontStyle": "italic" }
	  }, {
			"name": "Keyword", "scope": [
				"keyword.control",
				"keyword.type",
				"keyword.other",
				"constant.language",
				"storage.modifier",
				"support.function.builtin",
				"support.class.builtin",
				"support.type.builtin",
				"support.type.python",
				"support.other.builtin",
				"keyword.operator.logical.python",
				"storage.type.class.python",
				"storage.type.function",
				"keyword.operator.logical.python"
			],
			"settings": { "foreground": "#60A0E0", "fontStyle": "italic bold", }
		}, {
			"name": "Regular expression", "scope": ["string.regexp", "*.regexp"],
			"settings": { "foreground": "#A060F0" }
		}, {
			"name": "Operator", "scope": ["keyword.operator.arithmetic", "keyword.operator.assignment", "punctuation.separator.colon.python"],
			"settings": { "foreground": "#ff2060", "fontStyle": "bold" }
		}, {
			"name": "Punctuation separators", "scope": ["punctuation.section", "punctuation.separator.arguments", "punctuation.separator.parameters", "punctuation.separator.colon"],
			"settings": { "foreground": "#ff2060", "fontStyle": "bold" }
		}, {
			"name": "Punctuation", "scope": ["punctuation.separator", "punctuation.terminator", "punctuation.parenthesis", "punctuation.curlybrace", "punctuation.squarebracket"],
			"settings": { "foreground": "#FFFFFF" }
		}, {
		// 	"name": "Storage", "scope": "storage",
		// 	"settings": { "foreground": "#32E4D2", "fontStyle": "" }
		// }, {
		// 	"name": "Class type name", "scope": ["entity.name.type.class"],
		// 	"settings": { "foreground": "#32E4D2", "fontStyle": "bold underline" }
		// }, {
			"name": "Class name", "scope": ["entity.name.class"],
			"settings": { "foreground": "#32E4D2", "fontStyle": "bold underline" }
		}, {
			"name": "Storage type", "scope": "storage.type",
			"settings": { "foreground": "#32E4D2", "fontStyle": "" }
		}, {
			"name": "Type parameter", "scope": ["entity.name.type.type-parameter", "entity.type.type-parameter"],
		 	"settings": { "foreground": "#A0FF80", "fontStyle": "bold underline" }
		 }, {
		 	"name": "Local Variable name", "scope": ["entity.name.variable.local", "*.variable.local"],
		 	"settings": { "foreground": "#607090", "fontStyle": "" }
		 }, {
			"name": "Field name", "scope": "entity.name.variable.field",
			"settings": { "foreground": "#CD9F70", "fontStyle": "" }
		}, {
			"name": "Parameter", "scope": [ "variable.language.special" ],
			"settings": { "foreground": "#DC7223", "fontStyle": "" }
		}, {
			"name": "Preprocessor", "scope":["keyword.preprocessor", "meta.preprocessor"],
			"settings": { "foreground": "#80A080", "fontStyle": "italic" }
		}, {
			"name": "Function name", "scope": ["entity.name.function"],
			"settings": { "fontStyle": "", "foreground": "#ffffff" }
		}, {
			"name": "Function call", "scope": ["meta.function-call.generic"],
			"settings": { "fontStyle": "italic", "foreground": "#ffffff" }
		}, {
			"name":"Enum name", "scope":"entity.name.type.enum",
			"settings": { "foreground": "#A0FF80", "fontStyle": "" }
		}, {
			"name": "Function argument", "scope": ["variable.parameter", "*.variable.parameter"],
			"settings": { "foreground": "#DC7223", "fontStyle": "italic" }
		}, {
			"name": "Tag name", "scope": "entity.name.tag",
			"settings": { "foreground": "#F92672", "fontStyle": "" }
		}, {
			"name": "Tag attribute", "scope": "entity.other.attribute-name",
			"settings": { "foreground": "#A6E22E", "fontStyle": "" }
		}, {
			"name": "Support constant", "scope": ["support.constant"],
			"settings": { "foreground": "#ff5040", "fontStyle": "italic", }
		}, {
			"name": "Support library", "scope": ["support.function.magic"],
			"settings": { "foreground": "#40d0ff", "fontStyle": "italic bold underline", }
		}, {
			"name": "Library variable", "scope": "support.other.variable",
			"settings": { "fontStyle": "" }
		}, {
			"name": "Invalid", "scope": "invalid",
			"settings": { "foreground": "#ff0000", "fontStyle": "" }
		}, {
			"name": "Invalid deprecated", "scope": "invalid.deprecated",
			"settings": { "foreground": "#FF4000" }
		}, {
			"name": "diff.header", "scope": "meta.diff, meta.diff.header",
			"settings": { "foreground": "#75715E" }
		}, {
			"name": "diff.deleted", "scope": "markup.deleted",
			"settings": { "foreground": "#F92672" }
		}, {
			"name": "diff.inserted", "scope": "markup.inserted",
			"settings": { "foreground": "#A6E22E" }
		}, {
			"name": "diff.changed", "scope": "markup.changed",
			"settings": { "foreground": "#E6DB74" }
		}, {
			"name": "GitGutter deleted", "scope": "markup.deleted.git_gutter",
			"settings": { "foreground": "#F92672" }
		}, {
			"name": "GitGutter inserted", "scope": "markup.inserted.git_gutter",
			"settings": { "foreground": "#A6E22E" }
		}, {
			"name": "GitGutter changed", "scope": "markup.changed.git_gutter",
			"settings": { "foreground": "#E6DB74" }
		}, {
			"name": "GitGutter ignored", "scope": "markup.ignored.git_gutter",
			"settings": { "foreground": "#75715E" }
		}, {
			"name": "GitGutter untracked", "scope": "markup.untracked.git_gutter",
			"settings": { "foreground": "#75715E" }
		}, {
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": { "foreground": "#AE81FFA0" }
		}, {
			"scope": "entity.name.filename.find-in-files",
			"settings": { "foreground": "#E6DB74" }
		}, {
			"scope": "token.info-token",
			"settings": { "foreground": "#6796e6" }
		}, {
			"scope": "token.warn-token",
			"settings": { "foreground": "#cd9731" }
		}, {
			"scope": "token.error-token",
			"settings": { "foreground": "#f44747" }
		}, {
			"scope": "token.debug-token",
			"settings": { "foreground": "#b267e6" }
		}
	],
	"editor.tokenColorCustomizations": {
		"semanticHighlighting": true
	},
	"semanticTokenColors": {
		"namespace": "#d2ff9f",
		"class":{ "foreground": "#32E4D2"},
		"class.declaration":{ "foreground": "#32E4D2", "fontStyle": "bold underline" },
		"class.definition":{ "foreground": "#32E4D2", "fontStyle": "bold underline" },
		"interface": "#A0FF80",
		"type": "#32E4D2",
		"type.defaultLibrary":{ "foreground": "#60A0E0", "fontStyle": "italic bold" },
		"typeParameter": "#A0FF80",
		"enum": "#A0FF80",
		"enumMember": { "foreground": "#ff80b0", "fontStyle": "italic bold" },
		"number": "#ff80b0",
		"comment": "#48a723",
		"string": "#0080F0",
		"preprocessorText":{ "foreground": "#80A080", "background": "#003333", "fontStyle": "" },
		"preprocessorKeyword":{ "foreground": "#80A080", "background": "#003333", "fontStyle": "underline" },
		"keyword": { "foreground": "#60A0E0", "fontStyle": "italic bold", },
		"plainKeyword": { "foreground": "#60A0E0", "fontStyle": "italic bold", },
		"regexp": "#A060F0",
		"variable.readonly": "#ff5040",
		"variable.readonly.defaultLibrary": "#ff5040",
		"parameter": "#dc7223",
		"property": "#DACA80",
		"property.readonly": "#ff5040",
		"variable.local":"#607090",
		"local": "#607090"
	}
}
