{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		"editor.background": "#101010",
		"editor.foreground": "#C0C0C0",
		"editor.lineHighlightBackground": "#161616",
		"editor.selectionBackground": "#143D66",
		"editorCursor.foreground": "#f0f0f0",
		"editorIndentGuide.activeBackground": "#204060a0",
		"editorWhitespace.foreground": "#202020"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#60B040"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation.definition.arguments", "punctuation.definition.parameters"],
			"settings": {
				"foreground": "#C0C0C080",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#0080F0"
			}
		},
		{
			"name": "String comment",
			"scope": "string.quoted.docstring",
			"settings": {
				"foreground": "#48a723",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#ff80b0"
			}
		},
		{
			"name": "Number",
			"scope": "*.variable.enum-member",
			"settings": {
				"foreground": "#ff80b0"
			}
		},

		{
			"name": "Built-in constant",
			"scope": [],
			"settings": {
				"foreground": "#0080C0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "User-defined constant",
			"scope": ["constant.character", "constant.other"],
			"settings": {
				"foreground": "#A080C0",
				"fontStyle": ""
			}
		},
		// {
		// 	"name": "Variable",
		// 	"scope": "variable",
		// 	"settings": {
		// 		"foreground": "#A0C0ff",
		// 		"fontStyle": ""
		// 	}
		// },
		// {
		// 	"name": "Variable other",
		// 	"scope":"variable.other.readwrite",
		// 	"settings": {
		// 		"foreground": "#A0C0ff",
		// 		"fontStyle": ""
		// 	}
		// },
		{
			"name": "Variable other",
			"scope":"variable.other.object",
			"settings": {
				"foreground": "#9fffa7",
				"fontStyle": ""
			}
		},
		{
			"name":"Property",
			"scope":"variable.other.object.property",
			"settings": {
				"foreground": "#ffb759",
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "constant.language", "keyword.operator.logical.python"],
			"settings": {
				"foreground": "#60A0E0",
				"fontStyle": "italic bold",
			}
		},
		{
			"name": "Operator",
			"scope": ["keyword.operator", "punctuation.separator.colon.python"],
			"settings": {
				"foreground": "#ff2060",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Punctuation separators",
			"scope": ["punctuation.section", "punctuation.separator.arguments", "punctuation.separator.parameters", "punctuation.separator.colon"],
			"settings": {
				"foreground": "#ff2060",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation.separator", "punctuation.terminator", "punctuation.parenthesis", "punctuation.curlybrace", "punctuation.squarebracket"],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#32E4D2"
			}
		},
		{
			"name": "Storage modifier",
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#60A0E0",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#32E4D2"
			}
		},
		{
			"name": "Class name",
			"scope": ["entity.name.class", "entity.name.type.class"],
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#32E4D2"
			}
		},
		{
			"name": "Local Variable name",
			"scope": ["entity.name.variable.local", "*.variable.local"],
			"settings": {
				"foreground": "#5E94A2",
				"fontStyle": ""
			}
		},
		{
			"name": "Field name",
			"scope": "entity.name.variable.field",
			"settings": {
				"foreground": "#beffbc",
				"fontStyle": ""
			}
		},
		{
			"name": "Field name",
			"scope": "entity.name.variable.enum-member",
			"settings": {
				"foreground": "#fdff84",
				"fontStyle": ""
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"variable.language.special"
			],
			"settings": {
				"foreground": "#DC7223",
				"fontStyle": ""
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#35ffeb"
			}
		},
		{
			"name": "Preprocessor",
			"scope":["keyword.preprocessor", "meta.preprocessor"],
			"settings": {
				"foreground": "#008165",
				"fontStyle": "italic"
			}
		},
		// {
		// 	"name": "Function name",
		// 	"scope": ["entity.name.function"],
		// 	"settings": {
		// 		//"fontStyle": "bold",
		// 		"foreground": "#80e0b0"
		// 	}
		// },
		{
			"name": "Function call",
			"scope": ["meta.function-call.generic"],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Type name",
			"scope": "entity.name.type",
			"settings": {
				"fontStyle": "",
				"foreground": "#32E4D2"
			}
		},
		{
			"name":"Enum name",
			"scope":"entity.name.type.enum",
			"settings": {
				"foreground": "#B8D7A3",
				"fontStyle": ""
			}
		},
		{
			"name": "Function argument",
			"scope": ["variable.parameter", "*.variable.parameter"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#dc7223"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#F92672"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Support library",
			"scope": ["support.function", "support.constant", "support.type", "support.class"],
			"settings": {
				"foreground": "#60d0ff",
				"fontStyle": "italic bold",
			}
		},
		{
			"name": "Support library",
			"scope": ["support.function.magic"],
			"settings": {
				"foreground": "#60d0ff",
				"fontStyle": "italic bold underline",
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#FF4000"
			}
		},
		{
			"name": "diff.header",
			"scope": "meta.diff, meta.diff.header",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "GitGutter deleted",
			"scope": "markup.deleted.git_gutter",
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"name": "GitGutter inserted",
			"scope": "markup.inserted.git_gutter",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "GitGutter changed",
			"scope": "markup.changed.git_gutter",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"name": "GitGutter ignored",
			"scope": "markup.ignored.git_gutter",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"name": "GitGutter untracked",
			"scope": "markup.untracked.git_gutter",
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#AE81FFA0"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	],
	"editor.tokenColorCustomizations": {
		"semanticHighlighting": true
	},
	"editor.semanticTokenColorCustomizations": true, 
	"semanticTokenColors": {
		"namespace": "#d2ff9f",
		"type": "#32E4D2",
		"type.defaultLibrary":{
			"foreground": "#60c8e0",
			"fontStyle": "italic bold"
		},
		"class.defaultLibrary":{
			"foreground": "#60c8e0",
			"fontStyle": "italic bold"
		},
		"enum": "#B8D7A3",
		"enumMember": {
			"foreground": "#ff80b0",
			"fontStyle": "italic bold"
		},
		"method": "#d7bca3",
		"variable.readonly": "#ff5040",
		"variable.readonly.defaultLibrary": "#ff5040",
		"parameter": "#dc7223",
		"variable.local":"#5E94A2",
		"local": "#5E94A2"
	}
}
